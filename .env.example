# Yandex Cloud IAM токен (обязательно для работы)
# Получить: yc iam create-token
YANDEX_IAM_TOKEN=ваш_yandex_iam_токен

# Yandex Cloud Folder ID (обязательно для работы)
YANDEX_FOLDER_ID=ваш_folder_id

# Yandex Cloud API Key для YandexGPT
YANDEX_CLOUD_API_KEY=ваш_yandex_cloud_api_key

# Yandex Dictionary API Key
YANDEX_DICT_API_KEY=ваш_yandex_dict_api_key

# YDB (Yandex Database) настройки
YDB_ENDPOINT=grpcs://ydb.serverless.yandexcloud.net:2135
YDB_DATABASE=/ru-central1/ваш_folder_id/ваш_database_id

# Telegram Bot для авторизации (обязательно)
# Получить: https://t.me/BotFather -> /newbot
TELEGRAM_BOT_TOKEN=ваш_telegram_bot_token
TELEGRAM_BOT_USERNAME=ваш_bot_username